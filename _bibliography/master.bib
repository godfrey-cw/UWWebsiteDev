@article{grossMirrorSymmetryVarieties2017,
  title = {Towards Mirror Symmetry for Varieties of General Type},
  volume = {308},
  issn = {0001-8708},
  abstract = {The goal of this paper is to propose a theory of mirror symmetry for varieties of general type. Using Landau\textendash{}Ginzburg mirrors as motivation, we describe the mirror of a hypersurface of general type (and more generally varieties of non-negative Kodaira dimension) as the critical locus of the zero fibre of a certain Landau\textendash{}Ginzburg potential. The critical locus carries a perverse sheaf of vanishing cycles. Our main result shows that one obtains the interchange of Hodge numbers expected in mirror symmetry. This exchange is between the Hodge numbers of the hypersurface and certain Hodge numbers defined using a mixed Hodge structure on the hypercohomology of the perverse sheaf.},
  language = {en},
  journal = {Advances in Mathematics},
  doi = {10.1016/j.aim.2016.03.035},
