  abstract = {Schlichting conjectured that the negative K-groups of small abelian categories vanish and proved this for noetherian abelian categories and for all abelian categories in degree -1. The main results of this paper are that K-1(E) vanishes when E is a small stable {$\infty$}-category with a bounded t-structure and that K-n(E) vanishes for all n 1 when additionally the heart of E is noetherian. It follows that Barwick's theorem of the heart holds for nonconnective K-theory spectra when the heart is noetherian. We give several applications. On the one hand, we prove the vanishing results for the negative K-groups of a large class of dg algebras. On the other, we prove that various localization sequences `a la Blumberg-Mandell and Barwick-Lawson hold in nonconnective K-theory. This leads to the first computations in negative K-theory of nonconnective ring spectra arising in chromatic homotopy theory. For example, K-n(KU) vanishes for all n 1.},
  language = {en},
  journal = {arXiv:1610.07207 [math]},
  author = {Antieau, Benjamin and Gepner, David and Heller, Jeremiah},
  month = dec,
  year = {2018},
  keywords = {Mathematics - Algebraic Topology,Mathematics - K-Theory and Homology,16E45; 18E30; 19D35},
  file = {/home/charlie/Documents/Papers/Antieauet_al_2018_K-theoretic_obstructions_to_bounded_t-structures.pdf}
}

