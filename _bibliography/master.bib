@article{chanTropicalizationModuliSpace2016,
  title = {The Tropicalization of the Moduli Space of Curves {{II}}: {{Topology}} and Applications},
  abstract = {We study the topology of the tropical moduli space parametrizing stable tropical curves of genus g with n marked points in which the bounded edges have total length 1, and prove that it is highly connected. Using the identification of this space with the dual complex of the boundary in the moduli space of stable algebraic curves, we give a simple expression for the top weight cohomology of M\textsubscript{1,n} as a representation of the symmetric group and describe an explicit dual basis in homology consisting of abelian cycles for the pure mapping class group.},
  author = {Chan, Melody and Galatius, Soren and Payne, Sam},
  year = {2016},
  keywords = {Mathematics - Algebraic Geometry,Mathematics - Combinatorics,Mathematics - Geometric Topology,05c99,14c30,14h10,14t05,20f65,57q05}
}

@article{chanTropicalCurvesGraph2018,
  title = {Tropical Curves, Graph Homology, and Top Weight Cohomology of {{M}}{\textsubscript{g}}},
  abstract = {We study the topology of a space parametrizing stable tropical curves of genus g with volume 1, showing that its reduced rational homology is canonically identified with both the top weight cohomology of M\textsubscript{g} and also with the genus g part of the homology of Kontsevich's graph complex. Using a theorem of Willwacher relating this graph complex to the Grothendieck-Teichmueller Lie algebra, we deduce that H{$^{4g-6}$}(M\textsubscript{g};Q) is nonzero for g=3, g=5, and g at least 7. This disproves a recent conjecture of Church, Farb, and Putman as well as an older, more general conjecture of Kontsevich. We also give an independent proof of another theorem of Willwacher, that homology of the graph complex vanishes in negative degrees.},
  author = {Chan, Melody and Galatius, Soren and Payne, Sam},
  year = {2018},
  keywords = {Mathematics - Algebraic Geometry,Mathematics - Algebraic Topology,Mathematics - Geometric Topology}
}

@article{chanTopologyModuliSpaces2019,
  title = {Topology of Moduli Spaces of Tropical Curves with Marked Points},
  abstract = {We study a space of genus g stable, n-marked tropical curves with total edge length 1. Its rational homology is identified both with top-weight cohomology of the complex moduli space M\textsubscript{g,n} and with the homology of a marked version of Kontsevich's graph complex, up to a shift in degrees. We prove a contractibility criterion that applies to various large subspaces. From this we derive a description of the homotopy type of the tropical moduli space for g = 1, the top weight cohomology of M\textsubscript{1,n} as an S\textsubscript{n}-representation, and additional calculations for small (g,n). We also deduce a vanishing theorem for homology of marked graph complexes from vanishing of cohomology of M\textsubscript{g,n} in appropriate degrees, and comment on stability phenomena, or lack thereof.},
  author = {Chan, Melody and Galatius, Soren and Payne, Sam},
  year = {2019},
  keywords = {Mathematics - Algebraic Geometry,Mathematics - Algebraic Topology}
}

